{
  "name": "@consent-as-a-service/domain",
  "version": "1.0.0",
  "description": "Domain Models and Primitives for ConsentAsAService",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@js-joda/core": "^5.5.2"
  },
  "devDependencies": {
    "tsconfig": "workspace:*",
    "typescript": "^4.9.5",
    "@types/node": "^18.11.18",
    "jest": "28.1.3",
    "ts-jest": "28.0.8",
    "@types/jest": "28.1.8"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}